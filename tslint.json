{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "semicolon": [
            false,
            "never"
        ],
        "no-console": false,
        "no-var-requires": false,
        "no-shadowed-variable": false,
        "forin": false,
        "prefer-const": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}